<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
        <title>{% block title %}Happy Birds par NAO{% endblock %}</title>
        {% block stylesheets %}
            <!--[if lte IE 8]><script src="/web/js/ie/html5shiv.js"></script><![endif]-->
            <link rel="stylesheet" href="{{ asset('css/main.css') }}" />
            <!--[if lte IE 8]><link rel="stylesheet" href="/web/css/ie8.css" /><![endif]-->
            <!--[if lte IE 9]><link rel="stylesheet" href="/web/css/ie9.css" /><![endif]-->
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}

        <body>
        <div id="page-wrapper">

            <!-- Header -->
            <div id="header">

                <!-- Logo -->
                <h1><a href="index.html" id="logo">Happy Birds <em>par NAO</em></a></h1>

                <!-- Nav -->
                <nav id="nav">
                    <ul>
                        <li class="current"><a href="index.html">Accueil</a></li>
                        <li><a href="#">Consulter les observations</a></li>
                        <li><a href="#">Ajouter une observation</a></li>
                    </ul>
                </nav>

            </div>

            <!-- Banner -->
            <section id="banner">
                <header>
                    <h2>Happy Birds: <em>Une application pour partager votre passion des oiseaux</em></h2>
                    <!--<a href="#" class="button">Choisir une action...</a>-->
                </header>
            </section>



        </div>
        {% endblock %}
        {% block javascripts %}
            <!-- Scripts -->
            <script src="{{ asset('js/jquery.min.js') }}"></script>
            <script src="{{ asset('js/jquery.dropotron.min.js') }}"></script>
            <script src="{{ asset('js/skel.min.js') }}"></script>
            <script src="{{ asset('js/util.js') }}"></script>

            <!--[if lte IE 8]><script src=<script src="{{ asset('js/ie/respond.min.js') }}"></script>></script><![endif]-->

            <script src={{ asset('js/main.js') }}></script>
        {% endblock %}
    </body>
</html>






